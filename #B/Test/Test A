#include <iostream>
using namespace std;

class Car
{
private:
    int speed;
    int gear;
    string color;

public:
    int SetSpeed(int s)
    {
        speed = s;
        return speed;
    }

    int GetSpeed()
    {
        return speed;
    }
};

int main()
{
    Car myCar;
    myCar.SetSpeed(100);
    cout << "Speed : " << myCar.GetSpeed() << endl;
}
