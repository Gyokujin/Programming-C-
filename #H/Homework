#include <iostream>
using namespace std;

class Diagram
{
    int id, last_id;
public:
    Diagram() {}
    virtual void Draw() = 0;
    int GetID() { return id; }
};

class Point :public Diagram
{
    int x, y;
public:
    Point(int _x, int _y) :x(_x), y(_y) {}
    void Draw()
    {
        cout << "ì (" << x << "," << y << ")" << endl;
    }
};

class Line :public Diagram
{
    Point* p1;
    Point* p2;
public:
    Line(int x1, int y1, int x2, int y2)
    {

    }
    void Draw()
    {

    }

};

class Rectangle :public Diagram
{
    int bottom, left, right, top;
public:
    Rectangle(int l, int t, int r, int b)
    {

    }
    int GetArea()
    {

    }
    void Draw()
    {

    }
};

void main()
{
    Diagram* d[3];
    d[0] = new Point(3, 4);
    d[1] = new Line(0, 0, 5, 5);
    d[2] = new Rectangle(0, 0, 10, 10);

    for (int i = 0; i < 3; i++)
    {
        d[i]->Draw();
    }
}
